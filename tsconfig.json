{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "typeRoots": ["./types.ts", "node_modules/@types"],
        "strict": true,
        "paths": {
            "@/*": ["./*"]
        },
        "skipLibCheck": true,
        "lib": ["dom", "esnext"]
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ],
    "exclude": [
        "node_modules",
        "node_modules/**/*"
    ]
}
